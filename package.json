{
  "name": "tasco",
  "version": "0.0.0",
  "description": "Build tool for the HTML/CSS/JavaScript",
  "license": "MIT",
  "repository": "egoist/tasco",
  "author": {
    "name": "EGOIST",
    "email": "0x142857@gmail.com",
    "url": "github.com/egoist"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo lib/*.js && ./cli.js build"
  },
  "files": [
    "cli.js",
    "lib"
  ],
  "bin": "cli.js",
  "main": "lib/index.js",
  "keywords": [
    "build",
    "gulp",
    "html",
    "js",
    "css",
    "pack"
  ],
  "devDependencies": {
    "xo": "latest"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "operator-linebreak": [
        2,
        "before"
      ],
      "xo/no-process-exit": 0
    }
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-1": "^6.13.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.0",
    "cssnano": "^3.7.4",
    "dot-prop": "^3.0.0",
    "lodash.merge": "^4.6.0",
    "meow": "^3.7.0",
    "pify": "^2.3.0",
    "postcss": "^5.1.2",
    "precss": "^1.4.0",
    "pretty-hrtime": "^1.0.2",
    "pug": "^2.0.0-beta5",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sugarss": "^0.1.5"
  }
}
